{
  "name": "headers",
  "displayName": "_headers Language Support",
  "description": "Syntax highlighting support for the _headers file in Netlify/Cloudflare",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "headers",
        "aliases": [
          "Netlify _headers",
          "Cloudflare _headers"
        ],
        "filenames": [
          "_headers"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "headers",
        "scopeName": "source.headers",
        "path": "./syntaxes/headers.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "build": "node ./write-headers.mjs"
  }
}
