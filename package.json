{
  "name": "headers",
  "displayName": "_headers Language Support",
  "description": "Syntax highlighting support for the _headers file in Netlify/Cloudflare",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "headers",
      "aliases": ["Headers", "headers"],
      "extensions": [""],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "headers",
      "scopeName": "source.headers",
      "path": "./syntaxes/headers.tmLanguage.json"
    }]
  }
}
